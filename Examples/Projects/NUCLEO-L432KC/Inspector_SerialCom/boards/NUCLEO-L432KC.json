{
    "build": {
        "core": "stm32",
        "cpu": "cortex-m4",
        "extra_flags": [
            "-DSTM32L432xx",
            "-DSTM32L4xx",
            "-DUSE_HAL_DRIVER",
            "-DUSE_FULL_LL_DRIVER",
            "-DLUOSHAL=STM32L4 "
        ],
        "f_cpu": "80000000L",
        "mcu": "stm32l432kcu6",
        "product_line": "STM32L432xx"
    },
    "connectivity": [
        "can"
    ],
    "debug": {
        "jlink_device": "STM32L432KC",
        "openocd_target": "stm32l4x",
        "svd_path": "STM32L4x2.svd"
    },
    "frameworks": [
        "stm32cube"
    ],
    "name": "STM32L432",
    "upload": {
	    "maximum_ram_size": 65536,
	    "maximum_size": 262144,
        "protocol": "stlink",
        "protocols": [
            "stlink",
            "dfu"
        ]
    },
    "url": "https://luos.io",
    "vendor": "Luos"
}