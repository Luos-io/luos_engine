{
    "name": "Luos",
    "keywords": "robus,network,microservice,luos,operating system,os,embedded,communication,service,ST",
    "description": "Luos turns your embedded system into services like microservices architecture does it in software.",
    "version": "2.2.0",
    "authors": {
        "name": "Luos",
        "url": "https://luos.io"
    },
    "build": {
        "flags": [
            "-I inc",
            "-I Robus/inc",
            "-I OD",
            "-I Profiles/Core",
            "-I Profiles/State",
            "-I Profiles/Motor",
            "-I Profiles/Servo_motor",
            "-I Profiles/Voltage",
            "-I Bootloader"
        ],
        "srcFilter": [
            "+<*.c>",
            "+<../Robus/src/*.c>",
            "+<../Profiles/Core/*.c>",
            "+<../Profiles/State/*.c>",
            "+<../Profiles/Motor/*.c>",
            "+<../Profiles/Servo_motor/*.c>",
            "+<../Profiles/Voltage/*.c>",
            "+<../Bootloader/*.c>"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Luos-io/luos"
    },
    "dependencies": {
        "luos/LuosHAL": "^0.5.1"
    }
}
